    <div>
        <h2><%= pessoa.nome%></h2>
    
        <div>

            <div text=foto>
                <img src="https://avatars.githubusercontent.com/u/72801105?v=4" alt="Foto"
                width="200px"> 
            </div>
            

            <p><%= pessoa.descricao%></p>
        </div>
        <div>
            <p>CURSOS:</p>
            <% for (var i = 0; i < pessoa.cursos.length; i++) { %>
                <p><%= pessoa.cursos[i] %></li>
            <% } %>
        </div>
    </div>
</body>
